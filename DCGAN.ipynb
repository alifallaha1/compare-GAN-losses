   "metadata": {},
   "outputs": [],
   "source": [
    "import torch \n",
    "import torch .nn as nn \n",
    "import torch .optim as optim \n",
    "import torchvision\n",
    "import torchvision.datasets as datasets\n",
    "import albumentations as A\n",
    "from albumentations.pytorch import ToTensorV2\n",
    "from torch.utils.data import DataLoader , Dataset\n",
    "from torch.utils.tensorboard import SummaryWriter\n",
    "from models import Dicriminator , Genrator , init_weights\n",
    "from wgan import Critic ,Critic_GP,gradient_penalty\n",
    "import os\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "#from torchsummary import summary\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
    "batch_size = 128\n",
    "lr = 2e-4\n",
    "z_dim = 100\n",
    "channels = 3 \n",
    "filters = 64\n",
    "epochs = 1000 \n",
    "writer_real = SummaryWriter(f\"log/real\")\n",
    "writer_fake = SummaryWriter(f\"log/fake\")\n",
    "noise_test = torch.randn((batch_size,z_dim,1,1)).to(device)\n",
    "step=0\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "normalize= A.Compose(\n",
    "        [\n",
    "            A.Resize(height=64, width=64),\n",
    "            A.Normalize(\n",
    "                mean=[0.0, 0.0, 0.0],\n",
    "                std=[1.0, 1.0, 1.0],\n",
    "                max_pixel_value=255.0,\n",
    "            ),\n",
    "            ToTensorV2(),\n",
    "        ],\n",
    "    )\n",
    "\n",
    "path = \"data/\"\n",
    "image_files = [f for f in os.listdir(path) if f.endswith(('.png', '.jpg', '.jpeg'))]\n",
    "data = []\n",
    "\n",
    "for image in image_files:\n",
    "    full_path = os.path.join(path, image)\n",
    "    image = np.array(plt.imread(full_path))\n",
    "    normalized_image = normalize(image=image)['image']\n",
    "    data.append(normalized_image)\n",
    "      \n",
    "data_loader = DataLoader(data, batch_size=batch_size, shuffle=True) \n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "gen = Genrator (z_dim , channels , filters).to(device)\n",
    "disc=Dicriminator(channels , filters).to(device)\n",
    "init_weights(disc)\n",
    "init_weights(gen)\n",
    "opt_gen = optim.Adam(gen.parameters(),lr=lr ,betas=(0.5,0.999))\n",
    "opt_disc = optim.Adam(disc.parameters(),lr=lr ,betas=(0.5,0.999))\n",
    "criterion = nn.BCELoss()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for epoch in range(epochs):\n",
    "    if (epoch+1) % 50 == 0:\n",
    "        print(\n",
    "            f\"Epoch [{epoch+1}/{epochs}] Batch {batch_idx}/{len(data_loader)} \\\n",
    "                Loss D: {loss_disc:.4f}, loss G: {loss_gen:.4f}\"\n",
    "        )\n",
    "        with torch.no_grad():\n",
    "            fake = gen(noise_test)\n",
    "            
    "            img_grid_real = torchvision.utils.make_grid(real[:32], normalize=True)\n",
    "            img_grid_fake = torchvision.utils.make_grid(fake[:32], normalize=True)\n",
    "\n",
    "            writer_real.add_image(\"Real\", img_grid_real, global_step=step)\n",
    "            writer_fake.add_image(\"Fake\", img_grid_fake, global_step=step)\n",
    "    for batch_idx, real in enumerate(data_loader):\n",
    "        real = real.to(device)\n",
    "        noise = torch.randn((batch_size , z_dim , 1 ,1)).to(device)\n",
    "        fake = gen(noise)\n",
    "\n",
    "        disc_real = disc(real).reshape(-1)\n",
    "        loss_disc_real = criterion(disc_real , torch.ones_like(disc_real))\n",
    "        disc_fake = disc(fake.detach()).reshape(-1)\n",
    "        loss_disc_fake = criterion(disc_fake , torch.zeros_like(disc_fake))\n",
    "        loss_disc = (loss_disc_real+loss_disc_fake)/2\n",
    "        disc.zero_grad()\n",
    "        loss_disc.backward()\n",
    "        opt_disc.step()\n",
    "\n",
    "        output = disc(fake).reshape(-1)\n",
    "        loss_gen = criterion(output, torch.ones_like(output))\n",
    "        gen.zero_grad()\n",
    "        loss_gen.backward()\n",
    "        opt_gen.step()\n",
    "\n",
    "        step += 1\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#### wgan \n",
    "batch_size = 64 \n",
    "lr = 5e-5\n",
    "writer_real = SummaryWriter(f\"log/wganreal\")\n",
    "writer_fake = SummaryWriter(f\"log/wganfake\")\n",
    "crit=Critic(channels , filters).to(device)\n",
    "gen = Genrator (z_dim , channels , filters).to(device)\n",
    "init_weights(crit)\n",
    "init_weights(gen)\n",
    "opt_gen = optim.RMSprop(gen.parameters(),lr=lr)\n",
    "opt_crit = optim.RMSprop(crit.parameters(),lr=lr)\n",
    "crit_loop =5\n",
    "weight_clip = 0.01"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "## wgan \n",
    "\n",
    "for epoch in range(epochs):\n",
    "    print(epoch+1)\n",
    "    if (epoch+1) % 50 == 0:\n",
    "        print(\n",
    "            f\"Epoch [{epoch+1}/{epochs}] Batch {batch_idx}/{len(data_loader)} \\\n",
    "                Loss D: {loss_crit:.4f}, loss G: {loss_gen:.4f}\"\n",
    "        )\n",
    "        with torch.no_grad():\n",
    "            fake = gen(noise_test)\n",
    "           
    "            img_grid_real = torchvision.utils.make_grid(real[:32], normalize=True)\n",
    "            img_grid_fake = torchvision.utils.make_grid(fake[:32], normalize=True)\n",
    "\n",
    "            writer_real.add_image(\"Real\", img_grid_real, global_step=step)\n",
    "            writer_fake.add_image(\"Fake\", img_grid_fake, global_step=step)\n",
    "    for batch_idx, real in enumerate(data_loader):\n",
    "        real = real.to(device)\n",
    "        noise = torch.randn((batch_size , z_dim , 1 ,1)).to(device)\n",
    "        fake = gen(noise)\n",
    "        for _ in range(crit_loop):\n",
    "            crit_real = crit(real).reshape(-1)\n",
    "            crit_fake = crit(fake.detach()).reshape(-1)\n",
    "            loss_crit = -(torch.mean(crit_real) - torch.mean(crit_fake))\n",
    "            crit.zero_grad()\n",
    "            loss_crit.backward()\n",
    "            opt_crit.step()\n",
    "\n",
    "            for p in crit.parameters():\n",
    "                p.data.clamp_(-weight_clip , weight_clip)\n",
    "\n",
    "\n",
    "        output = crit(fake).reshape(-1)\n",
    "        loss_gen = -torch.mean(output)\n",
    "        gen.zero_grad()\n",
    "        loss_gen.backward()\n",
    "        opt_gen.step()\n",
    "\n",
    "        step += 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "## wgan gp\n",
    "batch_size = 64 \n",
    "lr = 1e-4\n",
    "writer_real = SummaryWriter(f\"log/wgan_GPreal\")\n",
    "writer_fake = SummaryWriter(f\"log/wgan_GPfake\")\n",
    "crit=Critic_GP(channels , filters).to(device)\n",
    "gen = Genrator (z_dim , channels , filters).to(device)\n",
    "init_weights(crit)\n",
    "init_weights(gen)\n",
    "opt_gen = optim.Adam(gen.parameters(),lr=lr ,betas=(0.0,0.9))\n",
    "opt_crit = optim.Adam(crit.parameters(),lr=lr ,betas=(0.0,0.9))\n",
    "crit_loop =5\n",
    "lambda_penalty = 10"
   ]
  },
